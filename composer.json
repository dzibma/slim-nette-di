{
    "name": "dzibma/slim-nette-di",
    "description": "Nette DI container for Slim framework",
    "keywords": ["slim", "nette"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=5.6",
        "slim/slim": "^3.9",
        "nette/di": "^2.4",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Dzibma\\Slim\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dzibma\\Slim\\Tests\\": "tests/"
        }
    }
}
